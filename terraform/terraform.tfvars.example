# Exemple de fichier terraform.tfvars
# Copiez ce fichier vers terraform.tfvars et remplissez les valeurs

aws_region        = "us-east-1"
environment       = "prod"
project_name      = "fl-crew-reddit"
s3_bucket_name    = "fl-crew-reddit-prod-1234567890"  # Doit Ãªtre unique globalement
ec2_instance_type = "g4dn.xlarge"  # Instance avec GPU
